{
    "spaces": [{
        "name": "newsfeed",
        "retentionPolicy": "inf",
        "shardDuration": "1h",
        "regex": "/^NEWSFEED\\..*/",
        "replicationFactor": 1,
        "split": 1
    }, {
        "name": "planet",
        "retentionPolicy": "24h",
        "shardDuration": "1h",
        "regex": "/^PLANET\\..*/",
        "replicationFactor": 1,
        "split": 1
    }, {
        "name": "container",
        "retentionPolicy": "24h",
        "shardDuration": "1h",
        "regex": "/^CONTAINER\\..*/",
        "replicationFactor": 1,
        "split": 1
    }, {
        "name": "min_rollup",
        "retentionPolicy": "30d",
        "shardDuration": "1h",
        "regex": "/^MIN\\..*/",
        "replicationFactor": 1,
        "split": 1
    }, {
        "name": "hour_rollup",
        "retentionPolicy": "30d",
        "shardDuration": "1h",
        "regex": "/^HOUR\\..*/",
        "replicationFactor": 1,
        "split": 1
    }],
    "continuousQueries": [
        "select MODE(txt_value) as txt_value, MAX(num_value) as num_value from /^CONTAINER\\..*$/ group by time(1m) into MIN.:series_name",
        "select MODE(txt_value) as txt_value, MAX(num_value) as num_value from /^CONTAINER\\..*$/ group by time(1h) into HOUR.:series_name",
        "select MODE(txt_value) as txt_value, MAX(num_value) as num_value from /^PLANET\\..*$/ group by time(1m) into MIN.:series_name"
    ]
}
